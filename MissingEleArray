public class Main
{
  public static int missingEle(int[] arr)
  {
    int len = arr.length+1;
    int[] newArr = new int[len+1];

    for(int i=0 ; i < len-1; i++)
    {
      newArr[arr[i]]++;
    }

    for(int i = 1; i <= len; i++)
    {
      if(newArr[i] == 0)
      {
        return i;
      }
    }
    return -1;
  }
  public static void main (String[] args)
  {
      int[] arr = {1,2,4,5,6};
      int res = missingEle(arr);
      System.out.println (res);
   }
}
